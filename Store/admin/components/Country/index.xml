<?xml version="1.0" standalone="no"?>
<!DOCTYPE swatml SYSTEM "http://swat.silverorange.com/swatml1.dtd">
<swatml>
	<widget class="SwatMessageDisplay" id="message_display" />
	<widget class="SwatFrame" id="index_frame">
		<property name="title" translatable="yes">Countries</property>
		<widget class="SwatToolbar" id="index_toolbar">
			<widget class="SwatToolLink">
				<property name="title" translatable="yes">New Country</property>
				<property name="link">Country/Edit</property>
				<property name="stock_id">create</property>
			</widget>
		</widget>
		<widget class="SwatForm" id="index_form">
			<widget class="SwatTableView" id="index_view">
				<object class="SwatTableViewCheckboxColumn" id="checkbox">
					<object class="SwatCheckboxCellRenderer" id="items">
						<property name="value" type="data">id</property>
					</object>
				</object>
				<object class="AdminTableViewOrderableColumn" id="title">
					<property name="title" translatable="yes">Title</property>
					<object class="AdminTitleLinkCellRenderer">
						<property name="text" type="data">title</property>
						<property name="link">Country/Edit?id=%s</property>
						<property name="link_value" type="data">id</property>
						<property name="stock_id">edit</property>
					</object>
				</object>
				<object class="AdminTableViewOrderableColumn" id="id">
					<property name="title" translatable="yes">Country Code</property>
					<object class="SwatTextCellRenderer">
						<property name="text" type="data">id</property>
					</object>
				</object>
				<object class="AdminTableViewOrderableColumn" id="show">
					<property name="title" translatable="yes">Show?</property>
					<object class="SwatBooleanCellRenderer">
						<property name="value" type="data">show</property>
					</object>
				</object>
			</widget>
			<widget class="SwatActions" id="index_actions">
				<widget class="SwatActionItem" id="delete">
					<property name="title" translatable="yes">deleteâ€¦</property>
				</widget>
			</widget>
		</widget>
	</widget>
</swatml>
