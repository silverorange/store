<?xml version="1.0" standalone="no"?>
<!DOCTYPE swatml SYSTEM "http://swat.silverorange.com/swatml1.dtd">
<swatml>
	<widget class="SwatMessageDisplay" id="message_display" />
	<widget class="SwatFrame" id="edit_frame">
		<property name="title" translatable="yes">Administrative Note</property>
		<widget class="AdminNote" id="admin_note">
			<property name="content" translatable="yes">This note is for administrative purposes and will not be displayed to the customer.</property>
		</widget>
		<widget class="SwatForm" id="edit_form">
			<widget class="SwatTextarea" id="notes" />
			<widget class="SwatFooterFormField">
				<widget class="SwatButton" id="submit_button" />
			</widget>
		</widget>
	</widget>
</swatml>
